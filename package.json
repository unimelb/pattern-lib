{
  "name": "@unimelb/pattern-lib-vue",
  "description": "A complete design system for the University of Melbourne.",
  "version": "4.9.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/unimelb/pattern-lib.git"
  },
  "author": "The University of Melbourne",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=10.0.0",
    "yarn": "*"
  },
  "main": ".out/vue.js",
  "files": [
    ".out/vue.js"
  ],
  "scripts": {
    "prebuild": "rm -Rf .out/docs",
    "prebuild:lib": "rm -Rf .out/lib",
    "build": "build-storybook -c .storybook -o .out/docs",
    "build:lib": "run-p build:lib:standard build:lib:minify",
    "build:lib:standard": "cross-env NODE_ENV=production webpack --config targets/lib/webpack.config.js",
    "build:lib:minify": "cross-env NODE_ENV=production MINIFY_CSS=true webpack --config targets/lib/webpack.minify.config.js",
    "build:vue": "cross-env NODE_ENV=production webpack --config targets/vue/webpack.config.js",
    "dev": "start-storybook -p 7002 -c .storybook",
    "start:lib": "webpack-dev-server --port 7003 --config targets/lib/webpack.config.js",
    "lint": "yarn lint:js && yarn lint:js --fix && yarn lint:css && yarn lint:css --fix",
    "lint:css": "stylelint \".storybook/**/*.css\" \"components/**/*.css\" \"targets/**/*.css\"",
    "lint:js": "eslint \"*.js\" \".storybook/**/*.{js,vue}\" \"components/**/*.{js,vue}\" \"targets/**/*.js\"",
    "test": "jest",
    "generate": "plop --plopfile generator/index.js",
    "prepublish": "cross-env LOAD_EXTERNAL_ASSETS=true npm run build:vue"
  },
  "dependencies": {
    "ike.js": "^0.0.2",
    "photoswipe": "^4.1.3",
    "semver": "^5.6.0",
    "smoothscroll-polyfill": "^0.4.4",
    "vue": "2.5.16",
    "vue-progressive-image": "3.1.0",
    "vue-scrollto": "^2.11.0"
  },
  "peerDependencies": {
    "vue": "2.5.13"
  },
  "devDependencies": {
    "@storybook/addon-notes": "^3.4.8",
    "@storybook/addon-options": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/vue": "^3.4.8",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "caniuse-db": "~1.0.30000977",
    "change-case": "^3.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "es6-promise": "^4.2.8",
    "eslint": "5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-loader": "~2.1.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-vue": "^4.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inquirer-directory": "^2.1.0",
    "jest": "^23.4.1",
    "jest-axe": "^3.1.1",
    "jest-serializer-vue": "^2.0.2",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "plop": "^2.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-nested": "^3.0.0",
    "postcss-preset-env": "~5.2",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.2",
    "pretty": "^2.0.0",
    "raw-loader": "^0.5.1",
    "storybook-readme": "3.1.1",
    "strip-html-comments": "^1.0.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-property-sort-order-smacss": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "svg-fill-loader": "^0.0.8",
    "svg-sprite-loader": "^3.8.0",
    "svg-url-loader": "^2.3.3",
    "vue-html-loader": "^1.2.4",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.2.6",
    "vue-template-compiler": "2.5.16",
    "vue-test-utils": "^1.0.0-beta.11",
    "webpack": "^3.8.1",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": ">=3.1.11",
    "webpack-merge": "^4.1.3"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^\\.storybook/(.*)$": "<rootDir>/.storybook/$1",
      "^icons/(.*)$": "<rootDir>/components/icons/$1",
      "^vue$": "vue/dist/vue.js"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": []
  }
}
