{
  "name": "@unimelb/pattern-lib-vue",
  "description": "A complete design system for the University of Melbourne.",
  "version": "0.0.7",
  "repository": "unimelb/unimelb-design-system",
  "author": "The University of Melbourne",
  "license": "UNLICENSED",
  "engines": {
    "node": "~6.11.3",
    "yarn": "*"
  },
  "main": ".out/vue.js",
  "files": [
    ".out/vue.js"
  ],
  "scripts": {
    "prebuild": "rm -Rf .out/docs",
    "prebuild:lib": "rm -Rf .out/lib",
    "build": "build-storybook -c .storybook -o .out/docs",
    "build:lib": "NODE_ENV=production webpack --config targets/lib/webpack.config.js",
    "build:vue": "LOAD_EXTERNAL_ASSETS=true NODE_ENV=production webpack --config targets/vue/webpack.config.js",
    "start": "start-storybook -p 7002 -c .storybook",
    "start:lib": "webpack-dev-server --port 7003 --config targets/lib/webpack.config.js",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:css": "stylelint \".storybook/**/*.css\" \"components/**/*.css\" \"targets/**/*.css\"",
    "lint:js": "eslint \"*.js\" \".storybook/**/*.{js,vue}\" \"components/**/*.{js,vue}\" \"targets/**/*.js\"",
    "generate": "plop --plopfile generator/index.js",
    "prepublish": "LOAD_EXTERNAL_ASSETS=true yarn run build:vue"
  },
  "dependencies": {
    "ike.js": "^0.0.2",
    "vue": "^2.4.2",
    "vue-progressive-image": "^2.2.1",
    "vue-scrollto": "^2.7.8"
  },
  "peerDependencies": {
    "vue": "^2.4.2"
  },
  "devDependencies": {
    "@storybook/addon-knobs": "^3.2.8",
    "@storybook/addon-notes": "^3.2.12",
    "@storybook/addon-options": "^3.2.12",
    "@storybook/vue": "^3.2.8",
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-stage-3": "^6.24.1",
    "css-loader": "^0.28.1",
    "dotenv": "^4.0.0",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-vue": "^3.13.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "fr-accordion": "^1.0.6",
    "fr-dialogmodal": "^1.0.4",
    "html-webpack-plugin": "^2.30.1",
    "inquirer-directory": "^2.1.0",
    "plop": "^1.9.0",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^1.0.1",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^6.0.4",
    "pretty": "^2.0.0",
    "raw-loader": "^0.5.1",
    "storybook-readme": "^3.0.6",
    "style-loader": "^0.17.0",
    "stylelint": "^8.1.1",
    "stylelint-config-property-sort-order-smacss": "^2.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "svg-fill-loader": "^0.0.8",
    "svg-url-loader": "^2.1.1",
    "svgo-loader": "^1.2.1",
    "vue-loader": "^13.0.4",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
