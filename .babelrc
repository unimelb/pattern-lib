{
  "presets": [
    "stage-3",
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": true,
        "targets": {
          "browsers": [
            "> 1% in AU",
            "last 2 versions",
            "Firefox ESR",
            "not ie < 11",
            "iOS >= 8.4",
            "Safari >= 8",
            "Android >= 4.4"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "transform-vue-jsx"
  ],
  "env": {
    "test": {
      "presets": [
        "stage-3",
        [
          "env",
          {
            "targets": {
              "node": "current"
            }
          }
        ]
      ]
    }
  }
}
